<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- Apps -->

	<bean id="integratedreports.app.integrated.reports" factory-bean="kenyacore.appFactoryBean" factory-method="createInstance">
		<constructor-arg value="integratedreports.reports" />
		<constructor-arg value="Example" />
		<constructor-arg value="integratedreports/example/home.page" />
		<constructor-arg value="integratedreports:apps/example.png" />
		<constructor-arg value="700" />
	</bean>
	
</beans>
